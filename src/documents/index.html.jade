#post-list
  section.recent-posts
    each post,idx in getFilesAtPath("posts", [{date:-1}]).toJSON().splice(0, 10)
      label.label.label-default
        != post.date
      h2.title
        a(href=post.url)= post.title
      != post.contentRenderedWithoutLayouts
      hr